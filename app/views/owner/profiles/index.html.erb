<p id="notice"><%= notice %></p>

<h1>Listing Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th>Cpf</th>
      <th>Services</th>
      <th>Mobile</th>
      <th>Zipcode</th>
      <th>Address</th>
      <th>Number</th>
      <th>Complement</th>
      <th>Neighborhood</th>
      <th>City</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.company %></td>
        <td><%= profile.cpf %></td>
        <td><%= profile.services %></td>
        <td><%= profile.mobile %></td>
        <td><%= profile.zipcode %></td>
        <td><%= profile.address %></td>
        <td><%= profile.number %></td>
        <td><%= profile.complement %></td>
        <td><%= profile.neighborhood %></td>
        <td><%= profile.city %></td>
        <td><%= profile.state %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
